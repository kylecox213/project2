<nav class="navbar p-4">
  <h1>Welcome, {{candidate.firstName}} {{candidate.lastName}}</h1>
  <a href="/logout"><button class="btn">Logout</button></a>
</nav>



<div class="row mt-5" style="max-width: 1200px; margin:auto">
  <div class="col-md-6 m-auto">
    <div class="card card-body">
      <h1 class="text-center mb-3"><i class="fas fa-sign-in-alt"></i>Prospects</h1>
      <img id="card-logo" src="/imgs/hound.png">
      <hr>
      <table id="jobs-list" class="table-hover">
        <thead>
          <tr>
            <th scope="col">Company</th>
            <th scope="col">Position</th>
          </tr>
        </thead>
        <tbody>
          {{#each jobs}}
          <tr>
            <td>{{company}}</td>
            <td>{{position}}</td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>